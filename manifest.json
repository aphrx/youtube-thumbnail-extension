{
    "name": "Youtube Thumbnail Injector",
    "description": "Injects image into Youtube's 'Up Next' item to visualize",
    "version": "0.1.0",
    "manifest_version": 2,
    "icons": {
        "16": "./icons/icon-16.png",
        "32": "./icons/icon-32.png",
        "48": "./icons/icon-48.png",
        "128": "./icons/icon-128.png"
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab"
    ]
}